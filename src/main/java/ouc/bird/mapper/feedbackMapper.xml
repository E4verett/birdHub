        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ouc.bird.mapper.FeedbackMapper">
    <insert id="fbUpload" parameterType="ouc.bird.pojo.Feedback">
        insert into feedback (`time`,`content`)values(#{time}, #{content})
    </insert>
</mapper>